//To write permuation of elements from 1 to n with no 2 adjacent element having 1 difference
#include<iostream>
#include<unordered_set>
using namespace std;

int main(){
   int n;
   cin>>n;
   int a[n];
   for(int i=1;i<=n;i++){
      a[i-1]=i;
   }
   for(int i=0;i<n;i++)
      cout<<a[i];cout<<endl;
   for(int i=1;i<n;i+=2){
      cout<<a[i];
   }
   for(int i=0;i<n;i+=2){
      cout<<a[i];
   }
   
      return 0;

   }
